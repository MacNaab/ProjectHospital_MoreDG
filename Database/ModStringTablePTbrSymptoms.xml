	<GameDBLocalizedString>	<LocID>SYM_MOREDG_SPLENOMEGALY</LocID>				<Text>Esplenomegalia</Text>	</GameDBLocalizedString>
	<GameDBLocalizedString>	<LocID>SYM_MOREDG_HYPERCALCEMIA</LocID>				<Text>Hipercalcemia</Text>	</GameDBLocalizedString>
